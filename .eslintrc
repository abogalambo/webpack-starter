{
	"extends": [
		"eslint:recommended",
    	"plugin:react/recommended"
    ],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"env": {
		"node": true,
		"browser": true
	},
	"rules": {
		"eol-last": ["error", "always"]
	},
	"settings": {
    "react": {
			"version": "detect", // automatically picks the version you have installed.
    }
	}
}
